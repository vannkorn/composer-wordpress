{
  "repositories": [
    
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },

    {
      "type": "package",
      "package": {
        "name": "wordpress",
        "type": "webroot",
        "version": "4.6",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/4.6.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.0.0"
        }
      }
    }
  ],

  "require": {
    "php": ">=5.3.0",
    "wordpress": "4.6",
    
    "wpackagist-plugin/akismet": "3.1.11",
    "wpackagist-plugin/advanced-access-manager": "3.7.1",
    "wpackagist-plugin/advanced-custom-fields": "4.4.8",
    "wpackagist-plugin/custom-post-type-ui": "1.3.5",
    "wpackagist-plugin/ninja-forms": "2.9.56.2",
    "wpackagist-plugin/ninja-forms-layout": "1.3",
    "wpackagist-plugin/wordpress-importer": "0.6.1"
  },

  "extra": {
    "webroot-dir": "core",
    "webroot-package": "wordpress"
  }
}